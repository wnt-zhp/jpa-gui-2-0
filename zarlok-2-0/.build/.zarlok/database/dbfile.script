CREATE MEMORY TABLE WYPROWADZENIE(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,DATA_UTWORZENIA TIMESTAMP NOT NULL,DATA_WYPROWADZENIA TIMESTAMP NOT NULL,ILOSC_JEDNOSTEK NUMERIC NOT NULL,TYTULEM VARCHAR(255) NOT NULL,DANIE_ID BIGINT,PARTIA_ID BIGINT NOT NULL,CONSTRAINT FK787FB1D071C9638E FOREIGN KEY(DANIE_ID) REFERENCES DANIE(ID),CONSTRAINT FK787FB1D0B5ABC366 FOREIGN KEY(PARTIA_ID) REFERENCES PARTIA(ID))
