CREATE MEMORY TABLE CONFIG_ENTRY(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,DESCRIPTION VARCHAR(2000),EDITABLE BOOLEAN,NAME VARCHAR(100) NOT NULL,VALUE LONGVARBINARY,VISIBLE BOOLEAN)
CREATE MEMORY TABLE DZIEN(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,DATA DATE,ILOSC_INNYCH INTEGER,ILOSC_KADRY INTEGER,ILOSC_UCZESTNIKOW INTEGER,STAWKADZIENNA NUMERIC,CONSTRAINT SYS_CT_49 UNIQUE(DATA))
