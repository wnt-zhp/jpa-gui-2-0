CREATE MEMORY TABLE POSILEK(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,COSTSTRICT BOOLEAN,DODATKOWY BOOLEAN,ILOSC_INNYCH INTEGER,ILOSC_KADRY INTEGER,ILOSC_UCZESTNIKOW INTEGER,KOSZT NUMERIC,NAZWA VARCHAR(255),DZIEN_ID BIGINT NOT NULL,CONSTRAINT FK4BE25D3D9393F52E FOREIGN KEY(DZIEN_ID) REFERENCES DZIEN(ID))
CREATE MEMORY TABLE POSILEK_DANIE(POSILEK_ID BIGINT NOT NULL,DANIA_ID BIGINT NOT NULL,CONSTRAINT SYS_CT_66 UNIQUE(DANIA_ID),CONSTRAINT FK3ECD1ECB71C79212 FOREIGN KEY(DANIA_ID) REFERENCES DANIE(ID),CONSTRAINT FK3ECD1ECB5AD0908E FOREIGN KEY(POSILEK_ID) REFERENCES POSILEK(ID))
